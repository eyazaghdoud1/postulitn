<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox fx:id="rlayout1" prefHeight="42.0" prefWidth="1818.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.RController">
   <children>
      <Label fx:id="lrole" prefHeight="26.0" prefWidth="217.0" text="Role" textFill="#0e1947">
         <HBox.margin>
            <Insets left="10.0" top="5.0" />
         </HBox.margin>
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="fieldmodif" prefHeight="26.0" prefWidth="121.0">
         <HBox.margin>
            <Insets left="10.0" top="9.0" />
         </HBox.margin>
      </TextField>
      <Button fx:id="modifbtn" mnemonicParsing="false" onAction="#ModifierRole" prefHeight="26.0" prefWidth="100.0" style="-fx-background-color: #0e1947;" text="Modifier" textFill="WHITE">
         <HBox.margin>
            <Insets left="1000.0" top="9.0" />
         </HBox.margin>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="suppbtn1" mnemonicParsing="false" onAction="#supprimerRole" prefHeight="26.0" prefWidth="100.0" style="-fx-background-color: #0e1947;" text="Supprimer" textFill="WHITE">
         <HBox.margin>
            <Insets left="80.0" right="30.0" top="9.0" />
         </HBox.margin>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</HBox>
