<HBox fx:id="rlayout1" prefHeight="42.0" prefWidth="552.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.RController">
   <children>
      <Label fx:id="lrole" prefHeight="26.0" prefWidth="249.0" text="Role">
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
         <font>
            <Font size="10.0" />
         </font>
      </Label>
      <TextField fx:id="fieldmodif" prefHeight="26.0" prefWidth="121.0">
         <HBox.margin>
            <Insets left="10.0" top="9.0" />
         </HBox.margin>
      </TextField>
      <Button fx:id="modifbtn" mnemonicParsing="false" onAction="#ModifierRole" prefHeight="26.0" prefWidth="115.0" style="-fx-background-color: #0e1947;" text="Modifier" textFill="WHITE">
         <HBox.margin>
            <Insets left="30.0" top="9.0" />
         </HBox.margin>
      </Button>
      <Button fx:id="suppbtn1" mnemonicParsing="false" onAction="#supprimerRole" prefHeight="26.0" prefWidth="121.0" style="-fx-background-color: #0e1947;" text="Supprimer" textFill="WHITE">
         <HBox.margin>
            <Insets left="10.0" right="20.0" top="9.0" />
         </HBox.margin>
      </Button>
   </children>
</HBox>
