      </ImageView>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label text="Java quiz">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <Label fx:id="nbQuestions" text="   Questions" />
         </children>
         <HBox.margin>
            <Insets left="20.0" />
         </HBox.margin>
         <padding>
            <Insets top="20.0" />
         </padding>
      </VBox>
      <Label text="Passer le quiz &gt;&gt; ">
         <HBox.margin>
            <Insets bottom="10.0" left="400.0" top="60.0" />
         </HBox.margin>
