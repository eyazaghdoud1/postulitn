<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<HBox prefHeight="77.0" prefWidth="774.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.TYPEITEMController">
   <children>
      <TextField fx:id="description">
         <HBox.margin>
            <Insets left="10.0" top="30.0" />
         </HBox.margin>
      </TextField>
      <Button mnemonicParsing="false" onAction="#modifier" style="-fx-background-color: #1E9DFF;" text="Modifier" textFill="WHITE">
         <HBox.margin>
            <Insets left="400.0" top="30.0" />
         </HBox.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#delete" style="-fx-background-color: red;" text="Supprimer" textFill="WHITE">
         <HBox.margin>
            <Insets left="80.0" top="30.0" />
         </HBox.margin>
      </Button>
   </children>
</HBox>
